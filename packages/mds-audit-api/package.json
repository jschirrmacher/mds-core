    "@mds-core/mds-db": "workspace:0.6.43",
    "@mds-core/mds-ingest-service": "0.14.1",
    "@mds-core/mds-logger": "workspace:0.5.9",
    "@mds-core/mds-provider-service": "workspace:0.3.7",
    "@mds-core/mds-schema-validators": "workspace:0.5.19",
